     <ENHMODE>D</ENHMODE>
     <FULL_NAME>\PR:CL_FPM_TREE_ASSISTANCE========CP\TY:LCL_EXPORT_ACTION\ME:EXECUTE\SE:BEGIN\EI</FULL_NAME>
     <SOURCE>
      <item>* additional export menu.</item>
      <item>    DATA: zlrt_result_data TYPE REF TO data,</item>
      <item>          zlt_columns TYPE CL_FPM_GUIBB_BASE_RENDER=&gt;T_COLUMN.</item>
      <item/>
      <item>    IF me-&gt;mv_format CP &apos;Z*&apos;.</item>
      <item>      zlt_columns = me-&gt;mo_tree_uibb_assist-&gt;mo_config_data-&gt;get_columns( iv_include_secondary_editors = abap_true ).</item>
      <item>      me-&gt;create_result_data(</item>
      <item>        EXPORTING</item>
      <item>          irt_columns                = ref #( zlt_columns )</item>
      <item>          iv_mc_image_src_field_name = me-&gt;get_master_col_image_src_field( )</item>
      <item>          irt_frontend_data          = me-&gt;get_frontend_data( )</item>
      <item>        RECEIVING</item>
      <item>          rrt_result_data            = zlrt_result_data</item>
      <item>      ).</item>
      <item>      zcl_a2xh_fpm_enh=&gt;enh_cl_fpm_list_uibb_assist_at(</item>
      <item>        EXPORTING</item>
      <item>          iv_format       = me-&gt;mv_format</item>
      <item>          irt_result_data = zlrt_result_data</item>
      <item>          it_p13n_column  = me-&gt;mo_tree_uibb_assist-&gt;mo_personalization_api-&gt;get_current_variant( )-&gt;get_columns( )</item>
      <item>          it_field_usage  = me-&gt;mo_tree_uibb_assist-&gt;mt_field_usage</item>
      <item>      ).</item>
      <item>      RETURN.</item>
      <item>    ENDIF.</item>
      <item/>
     </SOURCE>
